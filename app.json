  "logo": "https://graph.org/file/f382a449f798c7cc67a8b.jpg",
  "keywords": [
    "telegram",
    "string",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "repository": "https://github.com/deepaiimsss/SessionGenerator",
  "env": {
    "API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": ""
